{
  "name": "sovendus-integration-settings-ui",
  "version": "4.2.13",
  "license": "GPL-3.0-only",
  "private": false,
  "description": "Sovendus Integration Plugin Settings UI - React/tailwind based UI to configure Sovendus Integration Plugins",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "dist/logos/**/*",
    "src/**/*"
  ],
  "type": "module",
  "types": "./dist/package/index.d.ts",
  "main": "./dist/package/index.mjs",
  "module": "./dist/package/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/package/index.d.ts",
      "import": "./dist/package/index.mjs",
      "require": "./dist/package/index.cjs"
    },
    "./style-less": {
      "types": "./dist/package-style-less/index.d.ts",
      "import": "./dist/package-style-less/index.mjs",
      "require": "./dist/package-style-less/index.cjs"
    },
    "./demo": {
      "types": "./dist/demo/index.d.ts",
      "import": "./dist/demo/index.mjs",
      "require": "./dist/demo/index.cjs"
    },
    "./demo-style-less": {
      "types": "./dist/demo-style-less/index.d.ts",
      "import": "./dist/demo-style-less/index.mjs",
      "require": "./dist/demo-style-less/index.cjs"
    },
    "./raw-style.css": {
      "import": "./src/app.css",
      "require": "./src/app.css"
    },
    "./style.css": {
      "import": "./dist/package-style-less/sovendus-integration-settings-ui.css",
      "require": "./dist/package-style-less/sovendus-integration-settings-ui.css"
    },
    "./src/*": "./src/*",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "dev": "next dev",
    "build": "sovendus-builder build",
    "lint": "eslint --fix",
    "pub": "sovendus-release-tool"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sovendus-GmbH/sovendus-integration-settings-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Sovendus-GmbH/sovendus-integration-settings-ui/issues"
  },
  "homepage": "https://github.com/Sovendus-GmbH/sovendus-integration-settings-ui#readme",
  "keywords": [
    "sovendus",
    "integration",
    "types",
    "typescript"
  ],
  "author": "Marcus Brandstaetter <techsupport@sovendus.com>",
  "contributors": [
    {
      "name": "Marcus Brandstaetter - Sovendus GmbH",
      "email": "techsupport@sovendus.com"
    }
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.2.4",
